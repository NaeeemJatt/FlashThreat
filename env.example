# Flash Intelligence Environment Configuration
# Copy this file to .env and update the values

# Application Environment
APP_ENV=dev

# Database Configuration
POSTGRES_USER=flashthreat_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=flashthreat

# Security
JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters

# API Keys (Get these from the respective services)
VT_API_KEY=your_virustotal_api_key
ABUSEIPDB_API_KEY=your_abuseipdb_api_key
SHODAN_API_KEY=your_shodan_api_key
OTX_API_KEY=your_otx_api_key

# CORS Configuration
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:80"]

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Production Settings (for production deployment)
# APP_ENV=prod
# JWT_SECRET=your_production_jwt_secret_here
# POSTGRES_PASSWORD=your_production_database_password
